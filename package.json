{
  "name": "smil",
  "version": "2.8.0",
  "description": "Find the right emoji on the command-line",
  "keywords": [
    "cli-app",
    "cli",
    "emoji",
    "emojis",
    "search",
    "find",
    "matching",
    "relevant",
    "smil"
  ],
  "bin": {
    "smil": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jenbuzz/smil.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "lint": "xo",
    "test": "jest",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "clipboardy": "^2.3.0",
    "emojilib": "^2.4.0",
    "fuzzysearch": "^1.0.3",
    "inquirer": "^7.1.0",
    "inquirer-autocomplete-prompt": "^1.0.2",
    "meow": "^7.0.1"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "jest": "^26.0.1",
    "xo": "^0.32.0"
  },
  "xo": {
    "space": 4,
    "envs": [
      "node",
      "jest"
    ],
    "rules": {
      "promise/prefer-await-to-then": 0
    }
  }
}
